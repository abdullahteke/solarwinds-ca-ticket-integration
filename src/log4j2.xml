<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="SW-CA-TICKET-INTEGRATION" packages="">
  <Appenders>
    <File name="MyFile" fileName="logs/swCaTicketIntegration.log">
      <PatternLayout>
        <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Root level="debug">
      <AppenderRef ref="MyFile"/>
    </Root>
  </Loggers>
</Configuration>